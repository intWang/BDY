}

QCheckBox#chkClarity::indicator:unchecked:hover {
        image: url(:/Black/res/switch_left_hover);
}

QCheckBox#chkClarity::indicator:unchecked:pressed {
        image: url(:/Black/res/switch_left_press);
}

QCheckBox#chkFlip::indicator{
        width: 32px;
